permalink":"http://chenshuohuang.github.io/categories/%E6%88%90%E9%95%BF%E6%84%9F%E6%82%9F/"}],"tags":[{"name":"沟通","slug":"沟通","permalink":"http://chenshuohuang.github.io/tags/%E6%B2%9F%E9%80%9A/"},{"name":"人性","slug":"人性","permalink":"http://chenshuohuang.github.io/tags/%E4%BA%BA%E6%80%A7/"}]}],"categories":[{"name":"成长感悟","slug":"成长感悟","permalink":"http://chenshuohuang.github.io/categories/%E6%88%90%E9%95%BF%E6%84%9F%E6%82%9F/"}],"tags":[{"name":"试错","slug":"试错","permalink":"http://chenshuohuang.github.io/tags/%E8%AF%95%E9%94%99/"},{"name":"成长","slug":"成长","permalink":"http://chenshuohuang.github.io/tags/%E6%88%90%E9%95%BF/"},{"name":"沟通","slug":"沟通","permalink":"http://chenshuohuang.github.io/tags/%E6%B2%9F%E9%80%9A/"},{"name":"人性","slug":"人性","permalink":"http://chenshuohuang.github.io/tags/%E4%BA%BA%E6%80%A7/"}]}